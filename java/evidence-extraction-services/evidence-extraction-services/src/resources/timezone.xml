<?xml version="1.0" encoding="ISO-8859-1" ?>

<timezones>
    <timezone type="server"> <!--Se ejecutaran segun la hora del Server-->
        <name>United States of America summer time</name>
        <timezoneid>America/Los_Angeles</timezoneid>
        <day>1</day>
        <month>3</month>
        <dayofweek>1</dayofweek>
        <position>2</position>
        <hourofday>2</hourofday> <!-- este valor va de 0 a 23 -->
    </timezone>
    <timezone type="server"><!--Se ejecutaran segun la hora del Server-->
        <name>United States of America winter time</name>
        <timezoneid>America/Los_Angeles</timezoneid>
        <day>1</day>
        <month>11</month>
        <dayofweek>1</dayofweek>
        <position>1</position>
        <hourofday>2</hourofday><!-- este valor va de 0 a 23 -->
    </timezone>
    <timezone type="store"><!--Se ejecutaran segun la diferencia de horas entre server y store/s-->
        <name>United States of America summer time</name>
        <timezoneid>America/Santiago</timezoneid>
        <day>1</day>
        <month>3</month>
        <dayofweek>1</dayofweek>
        <position>2</position>
        <hourofday>02</hourofday><!-- este valor va de 0 a 23 -->
        <stores>
            <store>001</store>
            <store>002</store>
        </stores>
    </timezone>
    <timezone type="store"><!--Se ejecutaran segun la diferencia de horas entre server y store/s-->
        <name>United States of America winter time</name>
        <timezoneid>America/Santiago</timezoneid>
        <day>1</day>
        <month>11</month>
        <dayofweek>1</dayofweek>
        <position>1</position>
        <hourofday>2</hourofday><!-- este valor va de 0 a 23 -->
        <stores>
            <store>001</store>
            <store>002</store>
        </stores>
    </timezone>
</timezones>
